{
  "name": "makasim/values",
  "type": "library",
  "description": "An alter ego object. It is an object and an array at the same time",
  "keywords": ["model"],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksym Kotliar",
      "email": "kotlyar.maksim@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": { "Makasim\\Values\\": "src" },
    "files": ["src/functions_include.php"]
  },
  "autoload-dev": {
    "psr-4": { "Makasim\\Values\\Tests\\": "tests" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5-dev"
    }
  }
}
